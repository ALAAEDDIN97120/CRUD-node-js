<div class="container mt-5">
	<div class="row justify-content-center">
		<div class="col-md-8">
			<div class="card shadow-lg">
				<div class="card-header bg-warning text-black">
					<h2><%= data.title %></h2>
				</div>
				<div class="card-body">
					<p class="text-muted">
						Published At : <%= data.createdAt.toDateString() %>
					</p>
					<hr />
					<p><%= data.body %></p>
				</div>
			</div>
		</div>
	</div>
</div>
